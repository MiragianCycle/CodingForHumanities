; Working with numbers
(+ 5 3)    ; => 8
(- 10 4)   ; => 6
(* 2 6)    ; => 12
(/ 15 3)   ; => 5

; Working with strings
(string-append "Hello, " "Lisp!")   ; => "Hello, Lisp!"

; Working with symbols
(define my-symbol 'symbol-value)
my-symbol  ; => symbol-value

; Working with lists
(list 1 2 3 "four" 'five)  ; => '(1 2 3 "four" five)
